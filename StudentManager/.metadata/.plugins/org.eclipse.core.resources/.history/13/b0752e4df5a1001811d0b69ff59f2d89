package com.runsystem.datnt.models;

import java.io.Serializable;
import java.util.List;

import com.runsystem.datnt.entities.ResetPassword;
import com.runsystem.datnt.entities.User;

public class ResetPwdModel implements Serializable {

	private static final long serialVersionUID = 1L;
	private List<ResetPassword> unprocess;
	private List<ResetPassword> processed;
	
	public ResetPwdModel() {}

	public ResetPwdModel(User student, User admin) {
		super();
		this.student = student;
		this.admin = admin;
	}

	public User getStudent() {
		return student;
	}

	public void setStudent(User student) {
		this.student = student;
	}

	public User getAdmin() {
		return admin;
	}

	public void setAdmin(User admin) {
		this.admin = admin;
	}

	public static long getSerialversionuid() {
		return serialVersionUID;
	}
}
