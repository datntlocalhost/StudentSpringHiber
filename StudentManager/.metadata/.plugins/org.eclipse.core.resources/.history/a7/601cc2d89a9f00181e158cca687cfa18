package com.runsystem.datnt.daos.interfaces;

import java.util.List;

import com.runsystem.datnt.entities.Records;

public interface RecordsDao {
	
	public void             insert(RecordsDao record);      
	public void             update(RecordsDao record);
	public void             delete(Integer studentId);
	public RecordsDao       selectById(int studentId);
	public RecordsDao       selectByCode(String code);
	public RecordsDao       selectByName(String name);
	public List<RecordsDao> selectAll();
}
