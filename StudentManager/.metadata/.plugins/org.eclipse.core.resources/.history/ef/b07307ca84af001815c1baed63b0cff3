package com.runsystem.datnt.utils;

import lombok.AllArgsConstructor;
import lombok.Getter;
import lombok.NoArgsConstructor;

@Getter
@AllArgsConstructor
@NoArgsConstructor
public enum HeaderPackage {

	AUTH_REQUIREMENT("AUTH_REQUIREMENT", "", "/login"),
	CREATE_SUCCESS("CREATE_SUCCESS", "Create success"),
	DELETE_SUCCESS("DELETE_SUCCESS", "Delete success"),
	UPDATE_SUCCESS("UPDATE_SUCCESS", ""),
	CREATE_ERROR("CREATE_ERROR", ""),
	DELETE_ERROR("DELETE_ERROR", ""),
	UPDATE_ERROR("UPDATE_ERROR", "");
	
	private String info;
	private String message;
	private String redirectURI;
	
	private HeaderPackage(String info) {
		this.info = info;
	}
	
	private HeaderPackage(String info, String message) {
		this.info = info;
		this.message = message;
	}
}
