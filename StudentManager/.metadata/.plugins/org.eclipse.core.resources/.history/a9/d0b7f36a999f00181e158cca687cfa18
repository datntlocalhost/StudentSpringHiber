package com.runsystem.datnt.daos.interfaces;

import java.util.List;

import com.runsystem.datnt.entities.Student;

public interface StudentDao {

	public void         insert(Student school);      
	public void         update(Student school);
	public void         delete(Integer schoolId);
	public Student       selectById(int schoolId);
	public Student       selectByCode(String code);
	public Student       selectByName(String name);
	public List<Student> selectAll();
}
