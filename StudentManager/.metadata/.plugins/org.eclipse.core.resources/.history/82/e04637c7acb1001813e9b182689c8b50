    </Console>
  </Appenders>
  <Loggers>
    <Logger name="com.runsystem.datnt" level="info" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    
    <Root level="info">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>