package com.runsystem.datnt.services.interfaces;

import java.util.List;

import com.runsystem.datnt.entities.Student;
import com.runsystem.datnt.services.implementations.RoleServiceImpl;
import com.runsystem.datnt.services.implementations.SchoolServiceImpl;

public interface StudentService {
	
	public boolean       insert(Student student, RoleServiceImpl roleService, SchoolServiceImpl shoolService);      
	public void          update(Student student);
	public void          delete(Integer studentId);
	public Student       selectById(int studentId);
	public Student       selectByCode(String code);
	public Student       selectByName(String name);
	public List<Student> selectAll();
	public String        getMaxStudentCode();
}
